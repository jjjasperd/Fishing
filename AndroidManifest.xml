<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.Activity.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.Activity.GuideActivity"
            android:label="@string/title_activity_guide" >
        </activity>
        <activity
            android:name="com.Activity.SeaActivity"
            android:label="@string/title_activity_sea" >
        </activity>
        <activity
            android:name="com.Activity.Pacific"
            android:label="@string/title_activity_pacific" >
        </activity>
        <activity
            android:name="com.Activity.Atlantic"
            android:label="@string/title_activity_atlantic" >
        </activity>
        <activity
            android:name="com.Activity.Arctic"
            android:label="@string/title_activity_arctic" >
        </activity>
        <activity
            android:name="com.Activity.Indian"
            android:label="@string/title_activity_indian" >
        </activity>
        <activity
            android:name="com.Activity.Ranking"
            android:label="@string/title_activity_ranking" >
        </activity>
        <activity
            android:name="com.Activity.Book"
            android:label="@string/title_activity_book" >
        </activity>
    </application>

</manifest>